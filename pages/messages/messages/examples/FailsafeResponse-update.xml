<?xml version="1.0" encoding="UTF-8"?>
<!--This example is a Failsafe Message Response to a Failsafe Message with an identifier 6e5c9c23-6d60-4119-8569-9c0113a8cf45-->
<Bundle xmlns="http://hl7.org/fhir">
	<id value="5aafb39c-2978-492d-8ac5-df3e1ar9t3u8"/>
	<meta>
		<profile value="http://hl7.org/fhir/STU3/StructureDefinition/Bundle"/>
	</meta>
	<type value="message"/>
	<!--Entry for MessageHeader with id, timestamp, event type, source, responsible/publishing organization, communication-->
	<entry>
		<fullUrl value="urn:uuid:0191c2c6-8bec-472a-b039-9e35dc7e3s99"/>
		<resource>
			<MessageHeader>
				<id value="0191c2c6-8bec-472a-b039-9e35dc7e3s99"/>
				<meta>
					<versionId value="1"/>
					<!--when the message generated on Failsafe ##DateAndTimeRaised##-->
					<lastUpdated value="2020-07-28T15:00:33+00:00"/>
					<profile value="https://fhir.nhs.uk/STU3/StructureDefinition/Event-MessageHeader-1"/>
				</meta>
				<!--the standard routing demographics extension-->
				<extension url="https://fhir.nhs.uk/STU3/StructureDefinition/Extension-RoutingDemographics-1">
					<extension url="nhsNumber">
						<valueIdentifier>
							<system value="https://fhir.nhs.uk/Id/nhs-number"/>
							<value value="1112223330"/>
						</valueIdentifier>
					</extension>
					<extension url="name">
						<valueHumanName>
							<use value="official"/>
							<family value="DAWKINS"/>
							<given value="Sally"/>
						</valueHumanName>
					</extension>
					<extension url="birthDateTime">
						<valueDateTime value="2019-10-02T16:04:34+00:00"/>
					</extension>
				</extension>
				<!--the standard new/update/delete extension-->
				<extension url="https://fhir.nhs.uk/STU3/StructureDefinition/Extension-MessageEventType-1">
					<valueCodeableConcept>
						<coding>
							<system value="https://fhir.nhs.uk/STU3/CodeSystem/MessageEventType-1"/>
							<code value="update"/>
							<display value="Update event message"/>
						</coding>
					</valueCodeableConcept>
				</extension>
				<!--the ##FailsafeEventType##-->
				<event>
					<system value="https://fhir.nhs.uk/STU3/CodeSystem/EventType-1"/>
					<code value="failsafe-message-response-newborn-hearing-1"/>
					<display value="Failsafe Message Response Newborn Hearing"/>
				</event>
				<!--the time the message was sent - this could the same as meta.lateUpdated-->
				<timestamp value="2020-04-12T15:00:33+00:00"/>
				<!--standard contact details-->
								<source>
					<contact>
						<system value="phone"/>
						<value value="0191 1231234"/>
					</contact>
					<endpoint value="urn:nhs:addressing:asid:300000000161"/>
				</source>
				<!--a link to the responsible organisation-->
				<responsible>
					<reference value="urn:uuid:be6ff6b0-d299-495e-9919-ebc796b71020"/>
				</responsible>
				<!--focus links to the Task resource-->
				<focus>
					<reference value="urn:uuid:fb6fefa9-571a-40fd-a46a-0df2082c6381"/>
				</focus>
			</MessageHeader>
		</resource>
	</entry>
	<entry>
		<fullUrl value="urn:uuid:fb6fefa9-571a-40fd-a46a-0df2082c6381"/>
		<resource>
			<Task>
				<id value="fb6fefa9-571a-40fd-a46a-0df2082c6381"/>
				<meta>
					<profile value="https://fhir.hl7.org.uk/STU3/StructureDefinition/CareConnect-Task-1"/>
				</meta>
				<!--the Task.identifier is the unique identifier for this Failsafe Message Response ##Identifier##-->
				<identifier>
					<system value="https://supplierABC/identifiers"/>
					<value value="abc1111"/>
				</identifier>				
				<!--the Failsafe Message being responded to ##Original Failsafe Message Identifier##-->
				<groupIdentifier>
					<value value="6e5c9c23-6d60-4119-8569-9c0113a8cf45"/>
				</groupIdentifier>
				<!--fixed value for mandatory item-->
				<status value="received"/>
				<!--fixed value for mandatory item-->				
				<intent value="plan"/>
				<!--the event type is repeated here so that the task resource has all the necessary data-->
				<code>
					<coding>
						<system value="https://fhir.nhs.uk/STU3/CodeSystem/EventType-1"/>
						<code value="failsafe-message-response-newborn-hearing-1"/>
						<display value="Failsafe Message Response Newborn Hearing"/>
					</coding>
				</code>
				<!--a link to the Patient-->
				<for>
					<reference value="urn:uuid:1ca6b77f-5e2f-45a2-9ff9-0e0512c8e294"/>
				</for>				
				<!--the datetime the Failsafe Message Response was raised ##Date and Time created##-->
				<authoredOn value="2020-04-12T15:00:33+00:00"/>
				<!--the requester is the same as the responsible organisation in the MessageHeader-->
				<requester>
					<!--agent is the healthcare professional who raised the response-->
					<agent>
						<reference value="urn:uuid:69d845c8-67ad-4995-972b-008efc466a22"/>
					</agent>
					<!--onBehalfOf is the organisation that the healthcare professional raising the event represents-->
					<onBehalfOf>
						<reference value="urn:uuid:be6ff6b0-d299-495e-9919-ebc796b71020"/>
					</onBehalfOf>
				</requester>				
				<!--the reason the Failsafe Message Response was raised ##Failsafe Message Response Reason##-->
				<!--if no suitable code exists, then the text element of CodeableConcept should be used to carry an "other" reason as text-->
				<reason>
					<coding>
						<system value="https://fhir.nhs.uk/STU3/ValueSet/EMS-FailsafeResponseReason-1"/>
						<code value="other"/>
						<display value="Other - with mandatory text in the clinical record."/>
					</coding>
				</reason>
				<!--the date time from which NFMS should re-start Failsafe Messages for this trigger ##Time Period##-->
				<restriction>
					<period>
						<!--the date time can be represented as a date, or a date time-->
						<end value="2020-07-12"/>
					</period>
				</restriction>
			</Task>
		</resource>
	</entry>
	<entry>
		<fullUrl value="urn:uuid:be6ff6b0-d299-495e-9919-ebc796b71020"/>
		<resource>
			<!--the organisation that generated the Failsafe Message Response-->
			<Organization>
				<id value="be6ff6b0-d299-495e-9919-ebc796b71020"/>
				<meta>
					<profile value="https://fhir.hl7.org.uk/STU3/StructureDefinition/CareConnect-Organization-1"/>
				</meta>
				<identifier>
					<system value="https://fhir.nhs.uk/Id/ods-organization-code"/>
					<value value="01DAM"/>
				</identifier>
				<!--name of the organisation-->
				<name value="BURY ROAD SURGERY"/>
				<telecom>
					<system value="phone"/>
					<value value="0123 489 7854"/>
				</telecom>
			</Organization>
		</resource>
	</entry>
	<!--if the ##User## is a healthcare professional, then use CareConnect-Practitioner-1 to document their name & identifier-->
	<entry>
		<fullUrl value="urn:uuid:69d845c8-67ad-4995-972b-008efc466a22"/>
		<resource>
			<!--the healthcare professional raising the Failsafe Message Response-->
			<Practitioner>
				<id value="69d845c8-67ad-4995-972b-008efc466a22"/>
				<meta>
					<profile value="https://fhir.hl7.org.uk/STU3/StructureDefinition/CareConnect-Practitioner-1"/>
				</meta>
				<!--healthcare professional identifier-->
				<identifier>
					<system value="https://fhir.nhs.uk/Id/sds-user-id"/>
					<value value="048-r26"/>
				</identifier>
				<!--name of healthcare professional ##Forename## ##Surname##-->
				<name>
					<family value="WILKINS"/>
					<given value="Harry"/>
				</name>
			</Practitioner>
		</resource>
	</entry>
	<!--if the ##User## is a healthcare professional, then use CareConnect-PractitionerRole-1 to document their ##Practitioner Role##-->
	<entry>
		<fullUrl value="urn:uuid:e810b1e4-57c9-435e-a447-424d402c322f"/>
		<resource>
			<!--the healthcare professional raising the Failsafe Message Response-->
			<PractitionerRole>
				<id value="e810b1e4-57c9-435e-a447-424d402c322f"/>
				<meta>
					<profile value="https://fhir.hl7.org.uk/STU3/StructureDefinition/CareConnect-PractitionerRole-1"/>
				</meta>
				<!--healthcare professional identifier-->
				<practitioner>
					<reference value="urn:uuid:69d845c8-67ad-4995-972b-008efc466a22"/>
				</practitioner>
				<organization>
					<reference value="urn:uuid:be6ff6b0-d299-495e-9919-ebc796b71020"/>
				</organization>
				<code>
				<coding>
					<code value="R0260"/>
					<system value="https://fhir.hl7.org.uk/STU3/CodeSystem/CareConnect-SDSJobRoleName-1"/>
					<display value="General Medical Practitioner"/>
				</coding>
				</code>
			</PractitionerRole>
		</resource>
	</entry>
	<!--if the ##User## is a team, then use CareConnect-CareTeam-1 to document the ##Team##-->
	<entry>
		<fullUrl value="urn:uuid:08fcb778-6098-43bf-a32e-e16c3ebf2006"/>
		<resource>
			<!--the care team of the healthcare professional raising the Failsafe Message Response-->
			<CareTeam>
				<id value="08fcb778-6098-43bf-a32e-e16c3ebf2006"/>
				<meta>
					<profile value="https://fhir.hl7.org.uk/STU3/StructureDefinition/CareConnect-CareTeam-1"/>
				</meta>
				<name value="Child Care Red Team"/>
				<participant>
					<member>
						<reference value="urn:uuid:69d845c8-67ad-4995-972b-008efc466a22"/>
					</member>
				</participant>
				<managingOrganization>
					<reference value="urn:uuid:be6ff6b0-d299-495e-9919-ebc796b71020"/>
				</managingOrganization>
			</CareTeam>
		</resource>
	</entry>	
	<entry>
		<fullUrl value="urn:uuid:1ca6b77f-5e2f-45a2-9ff9-0e0512c8e294"/>
		<resource>
			<!--the demographic information-->
			<Patient>
				<id value="1ca6b77f-5e2f-45a2-9ff9-0e0512c8e294"/>
				<meta>
					<profile value="https://fhir.hl7.org.uk/STU3/StructureDefinition/CareConnect-Patient-1"/>
				</meta>
				<!--NHS number ##NHSNumber##-->
				<identifier>
					<extension url="https://fhir.hl7.org.uk/STU3/StructureDefinition/Extension-CareConnect-NHSNumberVerificationStatus-1">
						<valueCodeableConcept>
							<coding>
								<system value="https://fhir.hl7.org.uk/STU3/CodeSystem/CareConnect-NHSNumberVerificationStatus-1"/>
								<code value="01"/>
								<display value="Number present and verified"/>
							</coding>
						</valueCodeableConcept>
					</extension>
					<system value="https://fhir.nhs.uk/Id/nhs-number"/>
					<value value="1112223330"/>
				</identifier>
				<!--name ##Forename## ##Surname##-->
				<name>
					<use value="official"/>
					<family value="DAWKINS"/>
					<given value="Sally"/>
				</name>
				<!--Date of Birth (showing the optional birth time extension) ##DoB##-->
				<birthDate value="2019-10-02">
					<extension url="http://hl7.org/fhir/StructureDefinition/patient-birthTime">
						<valueDateTime value="2019-10-02T16:04:34+00:00"/>
					</extension>
				</birthDate>
			</Patient>
		</resource>
	</entry>	
</Bundle>
